{
  "extends": ["eslint-config-umi"],
  "rules": {
    "indent": [
      "error",
      "tab"
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "no-console": 0,
    "yield-star-spacing": ["error", {"before": true, "after": false}]
  }
}